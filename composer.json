{
    "name": "mickj/laravel-icon-generator",
    "description": "Comando Artisan per convertire SVG in componenti Blade Laravel, uniformando fill/stroke in modo intelligente.",
    "keywords": [
        "laravel",
        "icons",
        "svg",
        "blade",
        "generator",
        "artisan",
        "mickj"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michele De Palma",
            "email": "dm.elektro@libero.it"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "MickJ\\IconGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MickJ\\IconGenerator\\IconGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
